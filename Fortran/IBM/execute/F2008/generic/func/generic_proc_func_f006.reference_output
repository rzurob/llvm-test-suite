        -- inside fun_a10()
    inside func_fsub()
        -- inside fun_int()
    inside proc_fsub()
        -- inside sub_int()
    inside proc_ssub()
        -- inside fun_a10()
    inside func_ffunc()
        -- inside fun_int()
    inside proc_ffunc()
        -- inside sub_int()
    inside proc_sfunc()
