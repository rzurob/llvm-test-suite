#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc03.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#CREATOR = Bardia Mahjour
#DATE    = 20051031
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc03.f
#OUTPUT    = fxrtemsgalloc03.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc03.f
#OPTS      = -g
#STDERR    = fxrtemsgalloc03.out
#STDOUT    = fxrtemsgalloc03.out
#OUTPUT    = fxrtemsgalloc03$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc03$(OBJ)
#OUTPUT    = fxrtemsgalloc03$(EXE)
#ENDCMD

#CMD       = RUN
#ENVAR     = XLFRTEOPTS="errloc=yes"
#INPUT     = fxrtemsgalloc03$(EXE)
#STDOUT    = fxrtemsgalloc03.out
#STDERR    = fxrtemsgalloc03.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc03.out $($(tcasedir)fxrtemsgalloc03$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
