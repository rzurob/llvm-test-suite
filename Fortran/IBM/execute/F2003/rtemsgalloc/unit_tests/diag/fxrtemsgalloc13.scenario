#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc13.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#CREATOR = Bardia Mahjour
#DATE    = 20051031
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc13.f
#OUTPUT    = fxrtemsgalloc13.f
#ENDCMD

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc13a.f
#OUTPUT    = fxrtemsgalloc13a.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc13.f
#OPTS      = -c
#STDERR    = fxrtemsgalloc13.out
#STDOUT    = fxrtemsgalloc13.out
#OUTPUT    = fxrtemsgalloc13$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc13a.f
#OPTS      = -c
#STDERR    = fxrtemsgalloc13a.out
#STDOUT    = fxrtemsgalloc13a.out
#OUTPUT    = fxrtemsgalloc13a$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc13$(OBJ)
#INPUT     = fxrtemsgalloc13a$(OBJ)
#OUTPUT    = fxrtemsgalloc13$(EXE)
#ENDCMD

#CMD       = RUN
#ENVAR     = XLFRTEOPTS="errloc=yes"
#INPUT     = fxrtemsgalloc13$(EXE)
#STDOUT    = fxrtemsgalloc13.out
#STDERR    = fxrtemsgalloc13.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc13.out $($(tcasedir)fxrtemsgalloc13$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
