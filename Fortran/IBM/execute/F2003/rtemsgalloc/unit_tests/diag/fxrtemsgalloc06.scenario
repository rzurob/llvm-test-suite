#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc06.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#CREATOR = Bardia Mahjour
#DATE    = 20051028
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc06.f
#OUTPUT    = fxrtemsgalloc06.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc06.f
#OPTS      = -g
#STDERR    = fxrtemsgalloc06.out
#STDOUT    = fxrtemsgalloc06.out
#OUTPUT    = fxrtemsgalloc06$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc06$(OBJ)
#OUTPUT    = fxrtemsgalloc06$(EXE)
#ENDCMD

#CMD       = RUN
#ENVAR     = XLFRTEOPTS=errloc=yes
#INPUT     = fxrtemsgalloc06$(EXE)
#STDOUT    = fxrtemsgalloc06.out
#STDERR    = fxrtemsgalloc06.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc06.out $($(tcasedir)fxrtemsgalloc06$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
