#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc10.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#DATE    = 20051031
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc10.f
#OUTPUT    = fxrtemsgalloc10.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc10.f
#OPTS      = -g
#STDERR    = fxrtemsgalloc10.out
#STDOUT    = fxrtemsgalloc10.out
#OUTPUT    = fxrtemsgalloc10$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc10$(OBJ)
#OUTPUT    = fxrtemsgalloc10$(EXE)
#ENDCMD

#CMD       = RUN
#INPUT     = fxrtemsgalloc10$(EXE)
#STDOUT    = fxrtemsgalloc10.out
#STDERR    = fxrtemsgalloc10.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc10.out $($(tcasedir)fxrtemsgalloc10$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
