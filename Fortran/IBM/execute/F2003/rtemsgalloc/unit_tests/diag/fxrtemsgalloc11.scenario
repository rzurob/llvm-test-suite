#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc11.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#DATE    = 20051031
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc11.f
#OUTPUT    = fxrtemsgalloc11.f
#ENDCMD

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc11a.f
#OUTPUT    = fxrtemsgalloc11a.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc11.f
#OPTS      = -g
#STDERR    = fxrtemsgalloc11.out
#STDOUT    = fxrtemsgalloc11.out
#OUTPUT    = fxrtemsgalloc11$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc11$(OBJ)
#OUTPUT    = fxrtemsgalloc11$(EXE)
#ENDCMD

#CMD       = RUN
#ENVAR     = XLFRTEOPTS="errloc=yes"
#INPUT     = fxrtemsgalloc11$(EXE)
#STDOUT    = fxrtemsgalloc11.out
#STDERR    = fxrtemsgalloc11.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc11.out $($(tcasedir)fxrtemsgalloc11$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
