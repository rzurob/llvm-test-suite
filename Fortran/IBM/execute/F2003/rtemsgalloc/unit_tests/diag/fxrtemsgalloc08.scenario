#BEGINSCENARIO
#BEGINDOC
#NAME=fxrtemsgalloc08.scenario
#FEATURE = diagnostic for providing file name and line # when allocate/deallocate stmt fails at runtime.
#CREATOR = Bardia Mahjour
#DATE    = 20051031
#TEST_TYPE=diag test
#ENDDOC

#BEGINENV
#BEGINDEFS
TR_SRC=$(tcasedir)
COMPILER=$(tproddir)$(XLF_INVOC)
OPTIONS=$(F_COMPILE^OPT_SET_LANG_LANG_MSGS) $(? XOPTS)   $(COMP_LIBS)
#ENDDEFS
#ENDENV

#BEGINCONTROL

#CMD       = COPY
#INPUT     = $(tcasedir)fxrtemsgalloc08.f
#OUTPUT    = fxrtemsgalloc08.f
#ENDCMD

#CMD       = XLF90_COMPILE
#INPUT     = fxrtemsgalloc08.f
#OPTS      = -g
#STDERR    = fxrtemsgalloc08.out
#STDOUT    = fxrtemsgalloc08.out
#OUTPUT    = fxrtemsgalloc08$(OBJ)
#VALIDRCS  = 0
#ENDCMD

#CMD       = XLF90_LINK
#INPUT     = fxrtemsgalloc08$(OBJ)
#OUTPUT    = fxrtemsgalloc08$(EXE)
#ENDCMD

#CMD       = RUN
#ENVAR     = XLFRTEOPTS="errloc=yes"
#INPUT     = fxrtemsgalloc08$(EXE)
#STDOUT    = fxrtemsgalloc08.out
#STDERR    = fxrtemsgalloc08.out
#VALIDRCS  = $(RC_ANY)
#ENDCMD

#CMD       = DIFF
#INPUT     = fxrtemsgalloc08.out $($(tcasedir)fxrtemsgalloc08$(product)err)
#OPTS      = -b
#ENDCMD

#ENDCONTROL
#ENDSCENARIO
