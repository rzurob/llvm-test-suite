#OUTPUT    = dtpcompatdummyinkindlenpredtpmod.mod
#ENDCMD

#CMD       = XLF2003_COMPILE
#INPUT     = $(tcasedir)dtpCompatDummyInKindLenPostDTP.f
#OUTPUT    = dtpCompatDummyInKindLenPostDTP$(OBJ)
#ENDCMD

#CMD       = XLF2003_LINK
#INPUT     = dtpCompatDummyInKindLenPostDTP$(OBJ) $($(tcasedir)dtpCompatDummyInKindLenPreDTP.111$(Q32_64_MODE)$(product)$(OBJ))
